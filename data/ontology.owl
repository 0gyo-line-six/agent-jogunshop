<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/jogunshop#"
     xml:base="http://example.org/jogunshop"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://example.org/jogunshop"/>
    
    <!-- Classes -->
    <owl:Class rdf:about="#Product">
        <rdfs:label>Product</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#Delivery">
        <rdfs:label>Delivery</rdfs:label>
    </owl:Class>
    
    <owl:Class rdf:about="#General">
        <rdfs:label>General</rdfs:label>
    </owl:Class>
    
    <!-- Properties -->
    <owl:DatatypeProperty rdf:about="#hasPrice">
        <rdfs:label>hasPrice</rdfs:label>
        <rdfs:domain rdf:resource="#Product"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="#hasSize">
        <rdfs:label>hasSize</rdfs:label>
        <rdfs:domain rdf:resource="#Product"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    
</rdf:RDF>
